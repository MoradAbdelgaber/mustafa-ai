    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.46",
    "mongoose": "^5.9.13",
    "morgan": "^1.10.0",
    "node-jose": "^2.2.0",
    "node-machine-id": "^1.1.12",
    "nodemailer": "^6.10.0"
